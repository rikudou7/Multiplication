<?xml version='1.0' encoding='utf-8'?>
<widget id="com.multiply.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>multiply</name>
    <description>
        Multiplies two numbers together
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
	    <preference name="android-targetSdkVersion" value="28" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="android-build-tool" value="gradle" />
    <!--preference name="phonegap-version" value="cli-7.1.0" / -->
    <plugin name="cordova-plugin-admobpro" source="npm">
        <variable name="PLAY_SERVICES_VERSION" value="16.0.0" />
    </plugin>
    
</widget>
